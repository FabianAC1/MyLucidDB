<resourceBundle
	static="false"
	locale="en_US"
	exceptionClassName="net.sf.saffron.util.SaffronException">

<exception id="1" name="Internal">
    <text>Internal error: {0}</text>
</exception>

<exception id="2" name="ProgramCompilationError">
    <text>Compilation Error: {0}</text>
</exception>

<exception id="3" name="ProgramImplementationError">
    <text>Implementation Error: {0}</text>
</exception>

<exception id="4" name="CompilationAssertionError">
    <text>Assertion Error: {0}</text>
</exception>

<exception id="5" name="FunctionNotFound">
    <text>Function ''{0}'' does not exist</text>
</exception>

<exception id="6" name="ArgumentMustBeLiteral">
    <text>Argument to function ''{0}'' must be a literal</text>
</exception>

<exception id="7" name="ArgumentMustBePositiveInteger">
        <text>Argument to function ''{0}'' must be a positive integer</text>
    </exception>

<exception id="8" name="ValidationError">
    <text>Validation Error: {0}</text>
</exception>

<exception id="9" name="ParserError">
    <text>Parser Error: {0}</text>
</exception>

<exception id="10" name="ArgumentMustNotBeNull">
    <text>Argument to function ''{0}'' must not be NULL</text>
</exception>


<!-- ====================================================================== -->
<!-- Parser resources 100,000 - 110,000 -->
<!-- ====================================================================== -->
<message id="100000" name="ParserContext">
    <text>line {0,number,#}, column {1,number,#}</text>
</message>

<!-- ====================================================================== -->
<!-- Validator resources 120,000 - 130,000 -->
<!-- ====================================================================== -->
<exception id="120000" name="ValidatorUnknownFunction">
    <text>Reference to unknown function {0}; encountered near {1}</text>
</exception>
<exception id="120001" name="InvalidArgument">
    <text>Argument {0,number,#} to function ''{1}'' is invalid: {2}</text>
</exception>
<exception id="120002" name="InvalidNbrOfArgument">
    <text>Invalid number of arguments to function ''{0}''. Was expecting {1,number,#} arguments</text>
</exception>
<exception id="120010" name="CannotCastValue">
    <text>Cast function cannot convert value of type {0} to type {1}</text>
</exception>

<exception id="120011" name="UnknownTypeName">
    <text>Unknown type name: {0}</text>
</exception>

<!-- End of last error section ============================================ -->

</resourceBundle>

